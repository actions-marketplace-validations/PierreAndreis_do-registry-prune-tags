{
  "name": "do-registry-prune-tags",
  "version": "0.0.1",
  "private": true,
  "description": "Github Actions to prune old tags on a Container Registry hosted at Digital Ocean",
  "main": "src/main.ts",
  "scripts": {
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build src/main.ts --source-map --license licenses.txt -m",
    "test": "jest",
    "all": "yarn format && yarn lint && yarn test && yarn package",
    "dev": "nodemon src/main.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PierreAndreis/do-registry-prune-tags.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup",
    "digitalocean",
    "registry",
    "prune",
    "container"
  ],
  "author": "PierreAndreis",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "date-fns": "^2.22.1",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/date-fns": "^2.6.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.5",
    "@types/node-fetch": "^2.5.10",
    "@typescript-eslint/parser": "^4.28.1",
    "@vercel/ncc": "^0.28.6",
    "eslint": "^7.29.0",
    "eslint-plugin-github": "^4.1.3",
    "eslint-plugin-jest": "^24.3.6",
    "jest": "^27.2.4",
    "jest-circus": "^27.0.6",
    "jest-date-mock": "^1.0.8",
    "js-yaml": "^4.1.0",
    "msw": "^0.30.0",
    "nodemon": "^2.0.7",
    "prettier": "2.3.2",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4"
  }
}
